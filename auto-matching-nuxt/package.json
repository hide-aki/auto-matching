{
  "name": "auto-matching-nuxt",
  "version": "1.0.0",
  "description": "Auto Matching for Nuxt.js",
  "author": "tsu-nera",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "vuetype": "vuetype components",
    "vuetype:w": "vuetype -w components",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "eslint": "node_modules/.bin/eslint . --ext .vue,.js,.ts",
    "eslint:fix": "node_modules/.bin/eslint . --fix --ext .vue,.js,.ts"
  },
  "dependencies": {
    "eslint": "^5.10.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-vue": "^5.0.0",
    "@nuxtjs/axios": "^5.0.0",
    "cross-env": "^5.2.0",
    "nuxt": "^2.3.4",
    "ts-loader": "^5.3.1",
    "typescript": "^3.1.1",
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.15",
    "@vue/test-utils": "^1.0.0-beta.27",
    "jest": "^23.6.0",
    "nuxt-property-decorator": "^1.2.0",
    "phantomjs-prebuilt": "^2.1.16"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-loader": "^2.0.0",
    "nodemon": "^1.11.0",
    "prettier": "^1.15.3",
    "ts-jest": "^23.10.5",
    "typescript-eslint-parser": "^21.0.2",
    "vue-class-component": "^6.3.2",
    "vue-eslint-parser": "^4.0.3",
    "vue-jest": "^3.0.2",
    "vue-property-decorator": "^7.2.0",
    "vuetype": "^0.3.2",
    "vuex-class": "^0.3.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$"
  }
}
